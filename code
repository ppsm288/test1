public class GreetingApp {
    public static void main(String[] args) {
        greetWorld();
    }

    private static void greetWorld() {
        String greeting = "Hello, World!";
        System.out.println(greeting);
    }
}
cc
public class HelloWorld {
    public static void main(String[] args) {
        System.out.println("Hello, World!");
    }
}
